<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee" 
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee /WEB-INF/schema/web-facesconfig_2_0.xsd" 
              version="2.0" metadata-complete="false">
    <!-- This file must be present with our jar, even if it is empty.  Otherwise, our Java annotations won't get scanned! -->

<managed-bean>
<managed-bean-name>recipient</managed-bean-name>
<managed-bean-class>com.java.jsf.Recipient</managed-bean-class>
<managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>impl</managed-bean-name>
   <managed-bean-class>com.java.jsf.Impl</managed-bean-class>
   <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>paginationDao</managed-bean-name>
    <managed-bean-class>com.java.jsf.JsfPaginationBean</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>

<managed-bean>
	<managed-bean-name>claimPage</managed-bean-name>
    <managed-bean-class>com.java.jsf.ClaimPagination</managed-bean-class>
    <managed-bean-scope>request</managed-bean-scope>
</managed-bean>




</faces-config>